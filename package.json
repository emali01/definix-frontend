{
  "name": "definix-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "NODE_ENV=development react-scripts start",
    "build": "CI=false react-scripts build",
    "build:development": "NODE_ENV=development dotenv -e .env.development CI=false react-scripts build",
    "build:production": "NODE_ENV=production dotenv -e .env.production CI=false react-scripts build",
    "build:staging": "NODE_ENV=staging dotenv -e .env.staging CI=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "generate:config": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/generateConfig"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@binance-chain/bsc-use-wallet": "^0.8.1",
    "@crowdin/crowdin-api-client": "^1.10.4",
    "@ethersproject/abi": "^5.0.7",
    "@ethersproject/experimental": "^5.0.1",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.7",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.0",
    "canvas-confetti": "^1.3.3",
    "chart.js": "^2.9.4",
    "date-fns": "^2.17.0",
    "definixswap-sdk": "0.0.12",
    "definixswap-uikit": "^0.0.2",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "numeral": "^2.0.6",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-countup": "^4.3.3",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-ga": "^3.3.0",
    "react-gtm-module": "^2.0.11",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.1.1",
    "typescript": "^4.1.5",
    "web3": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pancakeswap-libs/eslint-config-pancake": "1.0.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.19",
    "@types/react-redux": "^7.1.16",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.20.0",
    "husky": "4.3.5",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^9.1.1"
  }
}
